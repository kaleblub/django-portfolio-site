{
  "version": 2,
  "builds": [
    {
      "src": "kaleblub_portfolio_django/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/vercel/path0/static/(.*)",
      "dest": "/vercel/path0/static/$1"
    },
    {
      "src": "/media/(.*)", 
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "kaleblub_portfolio_django/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}

