{
  "builds": [
    {
      "src": "kaleblub_portfolio_django/wsgi.py",
      "use": "@vercel/python",
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "kaleblub_portfolio_django/wsgi.py"
    }
  ]
}

